{
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register/transpile-only"
    ],
    "exclude": [
        "lib/",
        "**/*.d.ts",
        "coverage/",
        "test/"
    ],
    "reporter": [
        "text",
        "lcov"
    ],
    "cache": false,
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
}